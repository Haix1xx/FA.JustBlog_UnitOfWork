    @using FA.JustBlog.Core.Models;

@model IEnumerable<Category>
@foreach (var category in Model)
{
    <li class="nav-item" accesskey=""><a class="nav-link px-lg-3 py-3 py-lg-4" asp-controller="Post" asp-action="PostsByCategory" asp-route-category="@category.Name.ToLower()">@category.Name</a></li>
}
